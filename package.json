{
  "name": "express-npmcdn",
  "description": "Hosting npm package files",
  "files": [
    "lib",
    "docs"
  ],
  "scripts": {
    "start": "abigail test PKG",
    "test": "mocha --compilers js:espower-babel/guess",
    "cover": "nyc --reporter=lcov --reporter=text npm run test",
    "lint": "eslint src test",
    "compile": "babel -V && babel src --out-dir lib",
    "document": "jsdoc src -r README.md -d docs",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "dependencies": {
    "bluebird": "^3.3.3",
    "express": "^4.13.4",
    "gunzip-maybe": "^1.3.1",
    "mkdirp": "^0.5.1",
    "object-assign": "^4.0.1",
    "request": "^2.69.0",
    "tar-fs": "^1.11.1"
  },
  "devDependencies": {
    "abigail": "^0.1.2",
    "babel-cli": "^6.4.5",
    "babel-eslint": "^4.1.8",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "babel-register": "^6.4.3",
    "caravan": "^0.0.2",
    "del": "^2.2.0",
    "esdoc": "^0.4.5",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^4.0.0",
    "eslint-plugin-mocha": "^2.0.0",
    "espower-babel": "^4.0.1",
    "jsdoc": "^3.4.0",
    "mocha": "^2.4.5",
    "nyc": "^5.5.0",
    "power-assert": "^1.2.0",
    "semantic-release": "^4.3.5"
  },
  "keywords": [
    "express",
    "npm",
    "cdn"
  ],
  "author": "59naga <i59naga@icloud.com> (http://berabou.me/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/59naga/express-npmcdn.git"
  },
  "bugs": {
    "url": "https://github.com/59naga/express-npmcdn/issues"
  },
  "homepage": "https://github.com/59naga/express-npmcdn#readme"
}
